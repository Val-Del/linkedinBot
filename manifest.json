{
  "name": "ezLinkedin",
  "description": "Ajouter des contacts automatiquement sur Linkedin, Valentin Bruneel",
  "version": "2.0",
  "manifest_version": 3,
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [{
      "matches": ["https://www.linkedin.com/*"],
      "js": ["script.js"],
      "css": ["main.css"],
      "run_at": "document_end"
    }]
}
